{
  "expo": {
    "name": "quick-notes",
    "slug": "quick-notes",
    "version": "1.0.0",
    "orientation": "portrait",
    "entryPoint": "./index.tsx",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "cb73e917-1f20-41a8-b141-9b70427fd08c"
      }
    }
  }
}
